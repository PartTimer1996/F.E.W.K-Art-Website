<%- include("partials/header")-%>

<body class="text-center" cz-shortcut-listen="true">
<form class="form-signin" action="/Login" method="POST">
    <h1 class = "header">Hi there!</h1>
    <h1 class="h3 mb-3 font-weight-normal">Please sign in</h1>
    <label for="inputEmail" class="sr-only">Email address</label>
    <input type="email" name="username" id="inputEmail" class="form-control login" placeholder="Email address" required="true" autofocus="">
    <label for="inputPassword" class="sr-only">Password</label>
    <input type="password" name="password" id="inputPassword" class="form-control login" placeholder="Password" required="true">
    <div class="checkbox mb-3">
        <label>
            <input type="checkbox" value="remember-me"> Remember me
        </label>
    </div>
    <button class="btn btn-lg btn-dark btn-block" type="submit">Sign in</button>
    <p class="mt-5 mb-3 text-muted">New to F.E.W.K?<a href = "Register"> Click Here to Register</a></p>
</form>


</body>


<%- include("partials/footer")-%>
